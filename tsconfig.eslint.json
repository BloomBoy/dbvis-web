{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "paths": {
      "src/*": [
        "./*"
      ],
      "src": [
        "."
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    ".eslintrc.js",
    ".stylelint.js",
    "buildtimeData/.eslintrc.js",
    "buildtimeData/**/*.js",
    "buildtimeData/**/*.mjs",
    "tailwind.config.js",
    "next.config.js",
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.js",
    "src/**/*.jsx",
    "src/**/*.mjs",
    "src/**/*.mjsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
