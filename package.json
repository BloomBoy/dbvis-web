{
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon -w buildtimeData -w .env -w .env.\\* -w next.config.js -x \"next dev\"",
    "dev:prod": "nodemon -x \"cross-env NODE_ENV=production next build && cross-env NODE_ENV=production next start\"",
    "build": "next build",
    "start": "next start",
    "lint": "cross-env NODE_ENV=production run-p -cls --aggregate-output lint:*",
    "lint:eslint-check": "cross-env NODE_ENV=production eslint-config-prettier 'src/**/*.js'",
    "lint:prettier": "cross-env NODE_ENV=production prettier -c 'src/**/*.{js,jsx,ts,tsx,json}'",
    "lint:eslint": "cross-env NODE_ENV=production eslint src --report-unused-disable-directives --color",
    "lint:tsc": "cross-env NODE_ENV=production tsc --noEmit --pretty"
  },
  "nodemonConfig": {
    "ext": "js,jsx,ts,tsx,json,css,svg,mjs",
    "watch": [
      "buildtimeData",
      "src",
      "next.config.js",
      ".env"
    ]
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^15.12.1",
    "@contentful/rich-text-types": "^15.13.2",
    "axios": "^0.27.2",
    "classnames": "^2.3.2",
    "contentful": "^9.2.5",
    "contentful-management": "^10.15.0",
    "fast-safe-stringify": "^2.1.1",
    "jsonwebtoken": "^8.5.1",
    "next": "^12.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.1.8"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.7",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "@types/web-app-manifest": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "autoprefixer": "^10.4.11",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^12.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "postcss-import": "^15.0.0",
    "prettier": "^2.7.1",
    "tailwindcss-rtl": "^0.9.0",
    "typescript": "^4.8.3",
    "vercel": "^28.2.5"
  }
}
